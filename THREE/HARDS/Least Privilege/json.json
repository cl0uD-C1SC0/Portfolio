{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "1",
            "Effect": "Allow",
            "Action": [
                "ec2:DescribeVpcs"
            ],
            "Resource": "*"
        },
        {
            "Sid": "2",
            "Effect": "Allow",
            "Action": [
                "ec2:CreateFlowLogs"
            ],
            "Resource": "*"
        },
        {
            "Sid": "3",
            "Effect": "Allow",
            "Action": [
                "dynamodb:PutItem"
            ],
            "Resource": "arn:aws:dynamodb:us-east-1:569304435271:table/SecurityEventsTable"
        },
        {
            "Sid": "4",
            "Effect": "Allow",
            "Action": "sns:Publish",
            "Resource": ["arn:aws:sns:us-east-1:569304435271:SecurityEventsSnsTopic"]
        },
        {
            "Sid": "5",
            "Effect": "Allow",
            "Action": "iam:PassRole",
            "Resource": ["arn:aws:iam::569304435271:role/FlowLogsDeliveryRole"]
        }
    ]
}