{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Resource": "*",
            "Action": [
                "application-autoscaling:Describe*",
                "autoscaling:Describe*",
                "cloudformation:Describe*",
                "cloudformation:Get*",
                "cloudformation:List*",
                "cloudformation:Estimate*",
                "cloudformation:Preview*",
                "cloudtrail:Describe*",
                "cloudtrail:Get*",
                "cloudtrail:List*",
                "cloudtrail:LookupEvents",
                "cloud9:*",
                "cloudshell:*",
                "cloudwatch:Describe*",
                "cloudwatch:Get*",
                "cloudwatch:List*",
                "codebuild:Batch*",
                "codebuild:List*",
                "codebuild:StartBuild",
                "codebuild:StopBuild",
                "codecommit:BatchGet*",
                "codecommit:Get*",
                "codecommit:Git*",
                "codecommit:List*",
                "codecommit:TestRepositoryTriggers",
                "codepipeline:Acknowledge*",
                "codepipeline:DisableStageTransition",
                "codepipeline:EnableStageTransition",
                "codepipeline:Get*",
                "codepipeline:List*",
                "codepipeline:Poll*",
                "codepipeline:RetryStageExecution",
                "codepipeline:StartPipelineExecution",
                "ec2:Describe*",
                "ec2:Get*",
                "ec2messages:Get*",
                "ecr:BatchCheck*",
                "ecr:BatchGet*",
                "ecr:Describe*",
                "ecr:Get*",
                "ecr:List*",
                "ecs:Describe*",
                "ecs:List*",
                "elasticache:Describe*",
                "elasticache:List*",
                "elasticloadbalancing:Describe*",
                "events:Describe*",
                "events:List*",
                "events:Test*",
                "health:Describe*",
                "health:Get*",
                "health:List*",
                "iam:Get*",
                "iam:List*",
                "kms:Describe*",
                "kms:Get*",
                "kms:List*",
                "lambda:List*",
                "lambda:Get*",
                "logs:Describe*",
                "logs:Get*",
                "logs:FilterLogEvents",
                "logs:ListTagsLogGroup",
                "logs:TestMetricFilter",
                "s3:Get*",
                "s3:List*",
                "s3:Head*",
                "sns:Get*",
                "sns:List*",
                "sns:Check*",
                "sqs:Get*",
                "sqs:List*",
                "sqs:Receive*",
                "sts:Get*",
                "tag:Get*"
            ],
            "Effect": "Allow"
        },
        {
            "Resource": "arn:aws:cloudformation:us-east-1:*:stack/mod-modernizeourrent-service/*",
            "Action": [
                "cloudformation:Describe*",
                "cloudformation:Get*",
                "cloudformation:List*",
                "cloudformation:Estimate*",
                "cloudformation:Preview*",
                "cloudformation:CancelUpdateStack",
                "cloudformation:DeleteStack"
            ],
            "Effect": "Allow"
        },
        {
            "Resource": "*",
            "Action": [
                "codecommit:Delete*",
                "codecommit:CreateRepository",
                "codecommit:Update*",
                "codepipeline:Delete*"
            ],
            "Effect": "Deny"
        }
    ]
}